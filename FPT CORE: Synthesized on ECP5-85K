$ yosys -p "synth_ecp5 -top psi_fpt_core -json fpt_core.json" psi_fpt_core.v
$ nextpnr-ecp5 --85k --package CABGA381 --json fpt_core.json --lpf ecp5.lpf --textcfg fpt_core.config
$ ecppack --compress fpt_core.config fpt_core.bit
$ ecpprog fpt_core.bit
